# AWS S3 Configuration
# Copy this file to .env.local and fill in your actual values

# AWS Credentials - Get these from your AWS IAM console
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# AWS Region - The region where your S3 bucket is located
AWS_REGION=us-east-1

# S3 Bucket Name - The name of your S3 bucket
AWS_S3_BUCKET=your-bucket-name-here

# User ID for testing (can be any string)
USER_ID=user

# Next.js Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000